<%- include('partials/head') -%>
<%- include('partials/header') -%>

<br><br><br>
  <body id="top" class="flex  moving-gradient  flex-col text-center bg-gradient-to-r from-[#523015] via-[#a47739] to-[#996f36] mx-auto  w-full sm:px-6 lg:items-center">
    
 
          <section class="col-6 mt-5">
                <% if (locals.messages.errors) { %>
                    <% messages.errors.forEach( el => { %>
                        <div class="alert alert-danger"><%= el.msg %></div>
                    <% }) %>    
                <% } %>
                <% if (locals.messages.info) { %>
                    <% messages.info.forEach( el => { %>
                        <div class="alert alert-info"><%= el.msg %></div>
                    <% }) %>    
                <% } %>
 
            </section>

            
            <div class=" bg-gradient-to-l from-[#523015] via-[#a47739] to-[#996f36]  w-full py-16 px-4">  
              <div class="flex flex-col items-center justify-center">
                  
                
                  <div  class="bg-black/60 shadow rounded lg:w-1/3  md:w-1/2 w-full p-10 mt-16 ">
      <p tabindex="0" class="focus:outline-none text-2xl font-extrabold leading-6 text-white">Login to your account</p>
      <p tabindex="0" class="focus:outline-none text-sm mt-4 font-medium leading-none text-[#876c39]">Already have an account? <a href="/login"   class="hover:text-white focus:text-[#a47739] focus:outline-none focus:underline hover:underline text-sm font-medium leading-none  text-[#a47739] cursor-pointer"> Login here</a></p>
      <a href="/auth/google" aria-label="Continue with google" role="button" class="group text-white  bg-[#3d280f]   hover:bg-[#987842] hover:text-[#e4993e]  focus:outline-none focus:ring-2 focus:ring-offset-1 focus:ring-[#a16534]  py-3.5 px-4 border rounded-lg border-gray-700 flex items-center w-full mt-10 outline-none">
         
         <i class="fa-brands fa-google"></i>
          <p class="text-base font-medium ml-4 text-[#a47739] font-bold group-hover:text-white ">Continue with Google</p>
      </a>
      <!-- <a href="/auth/instagram" aria-label="Continue with github" role="button" class="group text-black bg-teal-500  hover:bg-teal-700 hover:text-white focus:outline-none focus:ring-2 focus:ring-offset-1 focus:ring-gray-700 py-3.5 px-4 border rounded-lg border-gray-700 flex items-center w-full mt-10 outline-none">
         <i class="fa-brands fa-instagram"></i>                          
          <p class="text-base font-medium ml-4 text-black font-bold group-hover:text-white">Continue with Instagram</p>
      </a> -->
      <a href="/auth/facebook" aria-label="Continue with facebook" role="button" class="group text-white  bg-[#3d280f]   hover:bg-[#987842] hover:text-[#e4993e]  focus:outline-none focus:ring-2 focus:ring-offset-1 focus:ring-[#a16534]  py-3.5 px-4 border rounded-lg border-gray-700 flex items-center w-full mt-10 outline-none">
        <i class="fa-brands fa-facebook  "></i>                        
          <p class="text-base font-medium ml-4 text-[#a47739] font-bold group-hover:text-white">Continue with Facebook</p>
      </a>
      
      <a onclick= 'unHide' aria-label="Continue with Email" role="button" class="group text-white  bg-[#3d280f]   hover:bg-[#987842] hover:text-[#e4993e]  focus:outline-none focus:ring-2 focus:ring-offset-1 focus:ring-[#a16534]  py-3.5 px-4 border rounded-lg border-gray-700 flex items-center w-full mt-10 outline-none">
        <i class="fas fa-envelope"></i> 
        <p class="text-base font-medium ml-4 text-[#a47739] font-bold group-hover:text-white">Continue with Email</p>
        
      </a>
      <div onclick="visible()" class="w-full flex items-center  justify-between py-5">
     
    
    
    </div>

            
             <form action="/signup" method="POST" class="">
                <div class="mb-3">
                    <label for="userName" class="form-label form-label text-lg font-lg leading-none text-white">User Name</label>
                    <input type="text" class="form-control" id="userName" name="userName">
                  </div>
                <div class="mb-3">
                  <label aria-labelledby="email" id="email" for="exampleInputEmail1" class="form-label text-lg font-lg leading-none text-white">Email Address</label>
                  <input type="email" class="form-control bg-gray-200 border rounded  text-xs font-medium leading-none text-gray-800 py-3 w-full pl-3 mt-2" id="exampleInputEmail1" aria-describedby="emailHelp" name="email">
                  <span id="emailHelp" class="form-text text-sm text-[#a47739]">We'll never share your email with anyone else.</span>
                </div>
                <div class="mb-3">
                  <label for="password" class="form-label text-lg font-lg leading-none text-white">Password</label>
                  <input id="pass" type="password" class="form-control bg-gray-200 border rounded  text-xs font-medium leading-none text-gray-800 py-3 w-full pl-3 mt-2" id="password" name="password">
                 
                </div>

                <div class="mb-3">
                    <label for="confirmPassword" class="form-label text-lg font-lg leading-none text-white ">Confirm Password</label>
                    <input type="password" class="form-control bg-gray-200 border rounded  text-xs font-medium leading-none text-gray-800 py-3 w-full pl-3 mt-2" id="confirmPassword" name="confirmPassword">
                </div>
               
                <div class="mt-8">
                  <button type="submit" role="button" class="border text-2xl font-bold border-2 border-white text-white bg-[#3d280f] hover:bg-[#987842] hover:text-[#e4993e] focus:ring focus:ring-[#a16534]  search-button hover:text-white py-4 px-3.5 text-lg w-full">Submit</button>
                </div>

              </form> 
        </div>
    </main>
  </div>
    </div>
</div>




<%- include('partials/footer') -%>
 <script src="/js/menuButton.js"></script>